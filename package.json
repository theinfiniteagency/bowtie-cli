{
  "name": "bowtie-cli",
  "description": "a scaffolding cli for The Infinite Agency projects",
  "version": "3.0.0",
  "author": "Casey Milstead @csymlstd",
  "bin": {
    "bowtie": "./bin/run"
  },
  "bugs": "https://github.com/theinfiniteagency/bowtie-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.4.10",
    "@oclif/config": "^1.4.10",
    "@oclif/plugin-help": "^1.2.3",
    "@oclif/plugin-warn-if-update-available": "^1.3.0",
    "axios": "^0.18.0",
    "chalk": "^2.3.2",
    "cheerio": "^1.0.0-rc.2",
    "cli-progress": "^1.8.0",
    "inquirer": "^5.2.0",
    "keychain": "^1.3.0",
    "limiter": "^1.1.2",
    "moment": "^2.22.0",
    "papaparse": "^4.3.7",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.9.16",
    "eslint": "^4.18.2",
    "eslint-config-oclif": "^1.4.0",
    "globby": "^8.0.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/src"
  ],
  "homepage": "https://github.com/theinfiniteagency/",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "bowtie",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available"
    ]
  },
  "repository": "https://github.com/theinfiniteagency/bowtie-cli/",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
